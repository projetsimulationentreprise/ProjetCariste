    body
        // entête
        .navbar-wrapper
            .container
                nav#navnav.navbar.navbar-inverse.navbar-static-top
                    .container
                        .navbar-header
                            button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
                                span.sr-only Toggle navigation
                                span.icon-bar
                                span.icon-bar
                                span.icon-bar
                            a.navbar-brand eRecrute
                        #navbar.navbar-collapse.collapse
                            ul.nav.navbar-nav
                                li
                                    a(href='index.html') Acceuil
                                li
                                    a(href='espace.html') Mon espace
                                li
                                    a(href='espace.html') Espace Recruteur
                                li
                                    a(href='#formul') Nous contacer
                                li
                                    a#log(href='#signup', data-toggle='modal', data-target='.bs-modal-sm') Se connecter
        // Fin entête
        img#navnav(src='../image/b1.jpg')
        // Login
        #myModal.modal.fade.bs-modal-sm(tabindex='-1', role='dialog', aria-labelledby='mySmallModalLabel', aria-hidden='true')
            .modal-dialog.modal-sm
                .modal-content
                    .bs-example.bs-example-tabs
                        ul#myTab.nav.nav-tabs
                            li.active
                                a(href='#signin', data-toggle='tab') Se connecter
                            li
                                a(href='#signup', data-toggle='tab') S'enregister
                    .modal-body
                        #myTabContent.tab-content
                            #signin.tab-pane.fade.active.in
                                form.form-horizontal
                                    fieldset
                                        // Se connecter
                                        // PSEUDO
                                        .control-group
                                            label.control-label(for='mail') Email:
                                            .controls
                                                input#userid.form-control(required='', name='userid', type='text', placeholder='Email@exemple.com')
                                        // MOT DE PASSE
                                        .control-group
                                            label.control-label(for='passwordinput') Mote de passe:
                                            .controls
                                                input#passwordinput.form-control(required='', name='passwordinput', type='password', placeholder='********')
                                        // Case à cocher
                                        .control-group
                                            label.control-label(for='rememberme')
                                            .controls
                                                label.checkbox.inline(for='rememberme-0')
                                                    input#rememberme-0(type='checkbox', name='rememberme', value='Remember me')
                                                    |                                                     Se rappeler
                                        // Button popup
                                        .control-group
                                            label.control-label(for='signin')
                                            .controls
                                                button#signin.btn.btn-success(name='signin') Se connecter
                            #signup.tab-pane.fade
                                form.form-horizontal
                                    fieldset
                                        // S'enregistrer
                                        // EMAIL
                                        .control-group
                                            label.control-label(for='mail') Email:
                                            .controls
                                                input#Email.form-control(name='mail', type='text', placeholder='@mail.com', required='mail')
                                        // PSEUDO
                                        .control-group
                                            label.control-label(for='userid') NOM:
                                            .controls
                                                input#userid.form-control(name='userid', type='text', placeholder='Nom', required='nom')
                                        // Mot DE PASSE
                                        .control-group
                                            label.control-label(for='password') Mot de passe:
                                            .controls
                                                input#password.form-control(name='password', type='password', placeholder='********', required='password')
                                                em 1-8 Characters
                                        // RE MOT DE PASSE
                                        .control-group
                                            label.control-label(for='reenterpassword') Re taper mot de passe:
                                            .controls
                                                input#reenterpassword.form-control(name='reenterpassword', type='password', placeholder='********', required='password')
                                         // localisation
                                        .control-group
                                            label.control-label(for='localisation') Localisation:
                                            .controls
                                                input#userid.form-control(name='localisation', type='text', placeholder='Paris, Lyon..', required='localisation')
                                        // skill
                                        .control-group
                                            label.control-label(for='skill') Expertise:
                                            .controls
                                                input#userid.form-control(name='skill', type='text', placeholder='Informatique, commerce...', required='skill')
                                        // move
                                        .control-group
                                            label.control-label(for='move') Etes vous pret a vous deplacer? :
                                            .controls
                                                input#userid.form-control(name='move', type='text', placeholder='oui, non, pourquoi..', required='move')

                                        // Pour Captcha
                                        // Button
                                        .control-group
                                            label.control-label(for='confirmsignup')
                                            .controls
                                                button#confirmsignup.btn.btn-success(name='confirmsignup') S'enregistrer
                    .modal-footer
                        center
                            button.btn.btn-default(type='button', data-dismiss='modal') Fermer
